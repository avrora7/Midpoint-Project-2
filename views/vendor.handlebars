<h1>View and Post a Job</h1>
<script src="/js/vendor.js"></script>

<table class="table table-striped table-bordered">
  <tr>
    <th>ID</th>
    <th>Date Posted</th>
    <th>Source language</th>
    <th>Target language</th>
    <th>Title</th>
    <th>Status</th>
    <th>Contact</th>

  </tr>
  {{#each jobs}}
    <tr>
      <td>{{id}}</td>
      <td>{{createdAt}}</td>
      <td>{{LanguageFrom.name}}</td>
      <td>{{LanguageTo.name}}</td>
      <td>{{title}}</td>
      <td>{{status}}</td>
      <td>{{User.fullName}}</td>
    </tr> 
  {{/each}}
</table>